TOOLS="~/tools"
PATH="$PATH:$HOME/bin:$HOME/.local/bin"

# custom functions
function config_add_all_previous(){
  cd ~/
  IFS=$'\n' FILES=($(config ls-tree -r master --name-only))
  for f in $FILES
  do
     config add $f
  done
  IFS=$' \t\n'
}
